{
  "compilerOptions": {
    "target": "ES5",
    "module": "ES2015",
    "moduleResolution": "Node",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "sourceMap": true,
    "allowJs": false,
    "jsx": "react",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true
  }
}
